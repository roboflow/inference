{
  "version": "1.0",
  "inputs": [
    {
      "type": "WorkflowImage",
      "name": "image"
    }
  ],
  "steps": [
    {
      "type": "roboflow_core/depth_estimation@v1",
      "name": "depth_estimation",
      "images": "$inputs.image",
      "model_version": "depth-anything-v2/small"
    }
  ],
  "outputs": [
    {
      "type": "JsonField",
      "name": "normalized_depth",
      "coordinates_system": "own",
      "selector": "$steps.depth_estimation.normalized_depth"
    },
    {
      "type": "JsonField",
      "name": "image",
      "coordinates_system": "own",
      "selector": "$steps.depth_estimation.image"
    }
  ]
}
